{
    "name": "@airbnb/ui",
    "version": "0.0.0",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "components",
        "icons",
        "hooks",
        "utils"
    ],
    "license": "MIT",
    "scripts": {
        "lint": "TIMING=1 eslint \"**/*.ts*\" --rule '@typescript-eslint/no-explicit-any: off'",
        "storybook": "concurrently \"yarn:watch:*\"",
        "build-storybook": "concurrently \"yarn:build:*\"",
        "build:css": "npx tailwindcss -i ./public/tailwind.css -o ./public/build.css",
        "build:storybook": "storybook build",
        "watch:storybook": "storybook dev -p 6006",
        "clean": "rimraf ./dist && rimraf node_modules",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "devDependencies": {
        "@storybook/addon-a11y": "^7.3.2",
        "@storybook/addon-actions": "^7.3.2",
        "@storybook/addon-essentials": "^7.3.2",
        "@storybook/addon-interactions": "^7.3.2",
        "@storybook/addon-links": "^7.3.2",
        "@storybook/addon-viewport": "^7.3.2",
        "@storybook/react": "^7.3.2",
        "@storybook/react-vite": "^7.3.2",
        "@storybook/testing-library": "^0.2.0",
        "@airbnb/eslint-config-custom-react": "*",
        "@airbnb/tsconfig": "*",
        "@types/node": "^20.5.1",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@vitejs/plugin-react": "^4.0.4",
        "concurrently": "^8.2.0",
        "eslint": "^8.47.0",
        "eslint-plugin-storybook": "^0.6.13",
        "next": "^13.4.19",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "^7.3.2",
        "typescript": "^5.1.6",
        "vite": "^4.4.9"
    },
    "dependencies": {
        "@testing-library/react": "^14.0.0",
        "blurhash": "^2.0.5",
        "class-variance-authority": "^0.7.0",
        "framer-motion": "^10.16.1",
        "happy-dom": "^10.10.4",
        "react-dropzone": "^14.2.3",
        "tailwindcss": "^3.3.3",
        "tiny-invariant": "^1.3.1",
        "vitest": "^0.34.2"
    }
}
